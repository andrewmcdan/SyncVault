{
  "name": "syncvault",
  "version": "0.1.0",
  "private": true,
  "description": "SyncVault tray app for templated config sync with GitHub and AWS Secrets Manager",
  "main": "dist/main/app/main.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\" \"npm run dev:electron\"",
    "dev:main": "tsc -p tsconfig.main.json --watch",
    "dev:renderer": "vite --config vite.renderer.config.ts",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json && node scripts/copy-schema.mjs",
    "build:renderer": "vite build --config vite.renderer.config.ts",
    "typecheck": "tsc -p tsconfig.renderer.json --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "vitest",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dev:electron": "wait-on dist/main/app/main.js http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron . --no-sandbox"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.0.0",
    "@octokit/rest": "^20.0.0",
    "chokidar": "^3.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "sql.js": "^1.12.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.0.0",
    "cross-env": "^7.0.3",
    "electron": "^30.0.0",
    "electron-builder": "^24.0.0",
    "eslint": "^9.0.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0",
    "vite": "^5.4.0",
    "vitest": "^1.0.0",
    "wait-on": "^7.0.1",
    "@types/sql.js": "^1.4.6"
  }
}
